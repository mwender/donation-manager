<div class="select-your-organization">
	{{#each rows}}
	<div class="row organization{{css_classes}}">
		<div class="col-lg-3 form-group">
			<a class="btn btn-primary btn-lg btn-block" href="{{link}}">{{button_text}}</a>
		</div>
		<div class="col-lg-9">
			<h3 style="margin-top: 0">{{name}}</h3>
			{{{desc}}}
		</div>
	</div>
	{{/each}}
</div>